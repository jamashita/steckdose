{
  "name": "@jamashita/steckdose",
  "version": "1.10.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/steckdose.git"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./config": {
      "import": {
        "types": "./dist/config/index.d.ts",
        "default": "./dist/config/index.js"
      }
    },
    "./digest": {
      "import": {
        "types": "./dist/digest/index.d.ts",
        "default": "./dist/digest/index.js"
      }
    },
    "./json": {
      "import": {
        "types": "./dist/json/index.d.ts",
        "default": "./dist/json/index.js"
      }
    },
    "./random": {
      "import": {
        "types": "./dist/random/index.d.ts",
        "default": "./dist/random/index.js"
      }
    },
    "./tokenizer": {
      "import": {
        "types": "./dist/tokenizer/index.d.ts",
        "default": "./dist/tokenizer/index.js"
      }
    },
    "./wait": {
      "import": {
        "types": "./dist/wait/index.d.ts",
        "default": "./dist/wait/index.js"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && tsc -b tsconfig.compilation.json",
    "clean": "rimraf dist",
    "commit": "git cz",
    "format": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "test": "vitest",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade && yarn audit fix --force",
    "update:upgrade": "ncu --packageManager yarn --deep --upgrade"
  },
  "dependencies": {
    "@jamashita/anden": "1.10.0",
    "bcrypt": "5.1.0",
    "mersenne-twister": "1.1.0",
    "yaml": "2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@jamashita/eslint-plugin": "1.83.0",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/bcrypt": "5.0.0",
    "@types/mersenne-twister": "1.1.2",
    "@types/node": "18.11.18",
    "git-cz": "4.9.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "lint-staged": "13.1.0",
    "npm-check-updates": "16.6.3",
    "rimraf": "4.1.2",
    "semantic-release": "20.1.0",
    "sort-package-json": "2.3.0",
    "typescript": "4.9.5",
    "vitest": "0.26.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  }
}
